<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.sydney.psov3">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/pos_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AdminActivity"
            android:label="Admin Priveleges" />
        <activity android:name=".Cashier" />
        <activity
            android:name=".ManageStaff"
            android:label="Manage Staff"
            android:parentActivityName=".AdminActivity" />
        <activity
            android:name=".ManageProduct"
            android:label="Manage Product"
            android:parentActivityName=".AdminActivity" />
        <activity
            android:name=".ManageJournal"
            android:label="Manage Journal"
            android:parentActivityName=".AdminActivity" />
        <activity
            android:name=".ManageProfile"
            android:label="My Profile"
            android:parentActivityName=".AdminActivity" />
    </application>

</manifest>